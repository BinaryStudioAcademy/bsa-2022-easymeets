{
  "ConnectionStrings": {
    "EasyMeetsCoreDBConnection": "Server=localhost;Database=EasyMeetsCoreDB;Trusted_Connection=True;"
  },
  "RabbitMQConfiguration": {
    "Uri": "amqp://guest:guest@localhost:5672",
    "Queues": {
      "InformConsumer": {
        "ExchangeName": "InformInfo",
        "ExchangeType": "direct",
        "RoutingKey": "receiver",
        "QueueName": "InformEveryone"
      },
      "InformProducer": {
        "ExchangeName": "InformInfo",
        "ExchangeType": "direct",
        "RoutingKey": "informer",
        "QueueName": "InformEveryone"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Firebase": {
      "ValidIssuer": "https://securetoken.google.com/easymeets-4ed60",
      "ValidAudience": "easymeets-4ed60"
    }
  },
  "ApplicationName": "EasyMeets"
}
